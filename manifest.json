{
  "manifest_version": 0.0,
  "name": "Personal Scheduler",
  "description": "This extension acts as a personal scheduler for a user",
  "version": "0.0",

  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["scheduler.js"]
    }
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
            "512": "schedulericon.png" }
}
