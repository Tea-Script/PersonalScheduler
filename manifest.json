{
  "manifest_version": 2,
  "name": "Personal Scheduler",
  "description": "This extension acts as a personal scheduler for a user",
  "version": "0.0",
  "content_security_policy": "script-src 'self'; object-src 'self'; media-src https://www.soundjay.com/button/beep-07.wav ",

  "background": [
    {
    "matches": ["<all_urls>", "http://ciaracoding.16mb.com/*"],
    "scripts": ["jquery-3.2.0.min.js" , "background.js"],
    "page" : "background.html"
    }
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "http://ciaracoding.16mb.com/*",
    "<all_urls>"

  ],
  "icons": {
    "512": "schedulericon.png" }
}
